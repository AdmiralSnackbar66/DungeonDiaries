    [
      "Venus",
      {
        "layout": "Numenera PC",
        "name": "Venus",
        "image": "numa-venus-token.png",
        "descriptor": "Mechanical",
        "type": "Nano",
        "focus": "Who Works miracles",
        "armor": 2,
        "tier": 1,
        "effort": 1,
        "xp": 0,
        "might": 7,
        "mightedge": 0,
        "speed": 11,
        "speededge": 0,
        "speedcost": 1,
        "intellect": 18,
        "intellectedge": 1,
        "abilities": [
          {
            "name": "Sense magic",
            "desc": "You can sense whether the numenera is active in situations where its presence is not obvious. You must study an object or location closely for a minute to get a feel for whether the touch of the ancients is at work."
          },
          {
            "name": "Hedge magic",
            "desc": "1 Intellect point. You can perform small tricks temporarily change the color or basic appearance of a small object cause small objects to float through the air clean a small area mend a broken object prepare (but not create) food "
          },
          {
            "name": "Healing Touch",
            "desc": "1 Intellect point. With a touch you restore 1d6 points to one stat Pool of any creature. This ability is a difficulty 2 Intellect task. Each time you attempt to heal the same creature the task difficulty increases by one step. The difficulty returns to 2 after that creature rests for ten hours. Action."
          },
          {
            "name": "Ward",
            "desc": "You have a shield of energy around you at all times that helps deflect attacks. You gain +1 to Armor. Enabler."
          }
        ],
        "cyphers": [
          {
            "name": "<STATBLOCK-LINK>Gravity-nullifying spray</STATBLOCK-LINK>",
            "desc": "Make a nonliving object float 1d20 feet in the air permanently."
          },
          {
            "name": "<STATBLOCK-LINK>Telepathy implant</STATBLOCK-LINK>",
            "desc": "Communicate telepathically with one creature."
          },
          {
            "name": "<STATBLOCK-LINK>Teleporter (bounder)</STATBLOCK-LINK>",
            "desc": "Teleport to a location you can see."
          }
        ],
        "skills": [
          {
            "name": "Trained in understanding numa-numa (Nano)",
            "desc": "You are trained in understanding numanera."
          },
          {
            "name": "Inability (Mechanical)",
            "desc": "You have a manner or an aura that others find a bit unnerving. Any task involving charm persuasion or deception is hindered."
          }
        ],
        "equipment": [
          {
            "name": "Clothing",
            "desc": ""
          },
          {
            "name": "Armor",
            "desc": "hides and furs"
          },
          {
            "name": "Book",
            "desc": "book about numa-numa"
          },
          {
            "name": "Weapon",
            "desc": "razor ring"
          }
        ],
        "attacks": [
          {
            "name": "Onslaught",
            "desc": "2 Intellect points. Attack a foe you see (physical/mental). If the attack is physical you emit a short-range ray of force that inflicts 4 points of damage. If the attack is mental you focus your mental energy to blast the thought processes of another creature within short range for 2 points of Intellect damage (ignores Armor). Action."
          },
          {
            "name": "Razor leaves",
            "desc": "2 physical damage (razor ring)."
          }
        ],
        "source": "Homebrew"
      }
